<h2>Wines</h2>
<p>
  <%= page_entries_info(@wines) %>
</p>
<ul start="">
  <% @wines.each_with_index do |wine, i| %>

      <li style="padding-bottom: 5px;">
        <strong><%= wine.name %></strong>: A <%= wine.varietal %> produced by <%= wine.winery %>.
        <br />For more info, <%= link_to('smell the cork', wine) %>.
      </li>
  <% end %>
</ul>
<%= paginate @wines %>